echo start
cd ~/src/nutro/
git stash
git pull
git stash pop
chmod +x ./gradlew
output=`./gradlew core:clean core:distZip`
if [ $? -eq 0 ]; then
        echo "build finished"
        cd ~/src/nutro/core/build/distributions
        unzip core-1.0.0-SNAPSHOT.zip
        cd core-1.0.0-SNAPSHOT
        cp -r ./* ~/deploy/back/
        echo "copied"
        echo "restarting"
        sudo docker restart back
        sudo docker logs --tail 100 -f back
else
        echo "build failed"
fi